{
  "kind": "collectionType",
  "collectionName": "blogpages",
  "info": {
    "singularName": "blogpage",
    "pluralName": "blogpages",
    "displayName": "blogpage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "headline": {
      "type": "string"
    },
    "pragraph": {
      "type": "text"
    },
    "date": {
      "type": "date"
    },
    "featuredImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "IsArticleHero": {
      "type": "boolean",
      "default": false
    },
    "IsItemsArticle": {
      "type": "boolean",
      "default": false
    },
    "slug": {
      "type": "string",
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
      "required": true,
      "unique": true
    }
  }
}
